import java.util.*;
public class Hello {

    public static void main(String[] args) {
Scanner sc=new Scanner(System.in);
String s=sc.nextLine();
for(int i=0;i<s.length();i++){
   int a=(int)(s.charAt(i)-96);
   for(int j=0;j<a;j++){
       System.out.print(s.charAt(i));
   }
   System.out.println();
}
}
}
