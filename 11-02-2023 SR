import java.util.*;
public class Hello {

    public static void main(String[] args) {
//Your Code Here
Scanner sc=new Scanner(System.in);
String s=sc.nextLine();
int n=sc.nextInt();
for(int i=0;i<n;i++){
   if(i==n-1){
       System.out.print(s.charAt(i));
   }
}
System.out.print(s.charAt(s.length()-n));

}
}
