import java.util.*;
public class Hello {

    public static void main(String[] args) {
		//Your Code Here
		Scanner sc=new Scanner(System.in);
		String S=sc.nextLine();
		int y=0;
		int n=0;
		for(int i=0;i<S.length();i++){
		    if(S.charAt(i)=='y'){
		        y+=1;
		    }else{
		        n+=1;
		    }
		}
		if(y>n){
		    System.out.println("YES");
		}else if(y<n){
		    System.out.println("NO");
		}else{
		    System.out.println("EQUAL");
		}

	}
}
