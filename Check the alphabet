import java.util.*;
public class Hello {

    public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		String S=sc.nextLine();
		int X=sc.nextInt();
		char m=(char)(96+X);
		int f=0;
		for(int i=0;i<S.length();i++){
		    if(S.charAt(i)==m){
		        f=1;
		    }
		}
		if(f==1){
		    System.out.print("yes");
		}else{
		    System.out.print("no");
		}

	}
}
