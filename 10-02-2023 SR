import java.util.*;
public class Hello {

    public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		String s=sc.nextLine();
		int count=0;
		for(int i=0;i<s.length();i++)
		{
		    if(!Character.isAlphabetic(s.charAt(i))&&!Character.isDigit(s.charAt(i)))
		    count++;
		}
		System.out.print(count);
	}
}
