import java.util.*;
public class Hello {

    public static void main(String[] args) {
		//Your Code Here
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int m=sc.nextInt();
		int a[][]=new int[n][m];
		for(int i=0;i<n;i++){
		    int sm = 0;
		    for(int j=0;j<m;j++){
		        a[i][j]=sc.nextInt();
		        sm += a[i][j];
		        System.out.print(a[i][j] + " ");
		    }
		    System.out.println(sm);
		}
	}
}
