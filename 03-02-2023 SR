import java.util.*;
public class Hello {

    public static void main(String[] args) {
		//Your Code Here
		Scanner sc= new Scanner(System.in);
		String a=sc.nextLine();
		System.out.print("(");
		for(int i=0;i<3;i++){
		    System.out.print(a.charAt(i));
		}
		System.out.print(") ");
		for(int j=3;j<9;j++){
		    if(j==6){
		    System.out.print(" ");
		    }
		   System.out.print(a.charAt(j));
		}

	}
}
