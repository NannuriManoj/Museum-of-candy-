import java.util.*;
public class Hello {

    public static void main(String[] args) {
		Scanner  sc=new Scanner(System.in);
		int n=sc.nextInt();
		if(n%2==0)
		{
		    System.out.print(n/100);
		}
		else
		{
		    int a=n/100;
		    int b=n/10%10;
		    int c=n%10;
		    System.out.print(a*100+(c*10+b));
		}
	}
}
