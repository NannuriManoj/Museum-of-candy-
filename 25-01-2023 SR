import java.util.*;
public class Hello {

    public static void main(String[] args) {
		//Your Code Here
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int a[]=new int[n];
		for(int i=0;i<n;i++){
		    a[i]=sc.nextInt();
		}
		for(int i=0;i<n;i+=2){
		    System.out.print(Math.min(a[i],a[i+1])+" ");
		    System.out.print(Math.max(a[i],a[i+1])+" ");
		}

	}
}
