import java.util.*;
public class Hello {

    public static void main(String[] args) {
//Your Code Here
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
int a[]=new int[n];
for(int i=0;i<n;i++){
   a[i]=sc.nextInt();
}
for(int i=0;i<n;i+=3){
   int x=a[i];
   int y=a[i+1];
   int z=a[i+2];
   System.out.print(z+" "+y+" "+x+" ");
}

}
}
